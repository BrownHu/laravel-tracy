{
    "name": "recca0120/laravel-tracy",
    "description": "Nette Tracy Debugger for Laravel 5",
    "keywords": ["laravel", "debugbar", "profiler", "debug", "webprofiler", "nette", "tracy"],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "tracy/tracy": "~2.3.5"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.2",
        "nesbot/carbon": "~1.20",
        "phpunit/phpunit": "~4.1"
    },
    "suggest": {
        "recca0120/terminal": "Required to use the Terminal Panel (~2.0.5)"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\LaravelTracy\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
